{
  "name": "react-js-lints-hooks-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "normalize-scss": "^7.0.1",
    "sass": "^1.47.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard-scss": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "eslint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "eslint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "prettier": "prettier . --check --ignore-path .gitignore",
    "prettier:fix": "prettier . --write --ignore-path .gitignore",
    "stylelint": "stylelint **/*.scss --ignore-path .gitignore",
    "stylelint:fix": "stylelint **/*.scss --fix --ignore-path .gitignore"
  }
}
